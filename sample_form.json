              "action": {
                "type": "setObjectKey",
                "varName": "user_profile",
                "objectKey": "email",
                "value": "2"
              }
            }
          ]
        },
        {
          "id": 3,
          "title": "Age",
          "type": "numeric",
          "value": "",
          "placeholder": "Enter your age",
          "onCondition": [
            {
              "condition": {
                "path": "3",
                "op": "isNotEmpty",
                "value": ""
              },
              "action": {
                "type": "setObjectKeyFromQuestion",
                "varName": "user_profile",
                "objectKey": "age",
                "value": ""
              }
            }
          ]
        },
        {
          "id": 4,
          "title": "Employment Status",
          "type": "choice",
          "value": "",
          "choices": ["employed", "student", "unemployed", "retired"],
          "onCondition": [
            {
              "condition": {
                "path": "4",
                "op": "equals",
                "value": "employed"
              },
              "action": {
                "type": "append",
                "varName": "user_tags",
                "value": "professional"
              }
            },
            {
              "condition": {
                "path": "4",
                "op": "equals",
                "value": "student"
              },
              "action": {
                "type": "append",
                "varName": "user_tags",
                "value": "education"
              }
            }
          ]
        },
        {
          "id": 5,
          "title": "Company Name",
          "type": "text",
          "value": "",
          "placeholder": "Enter your company name",
          "visibleWhen": {
            "path": "4",
            "op": "equals",
            "value": "employed"
          }
        },
        {
          "id": 6,
          "title": "Skills",
          "type": "toggleList",
          "value": "",
          "choices": ["JavaScript", "Python", "React", "Node.js", "Django", "Vue.js"],
          "onCondition": [
            {
              "condition": {
                "path": "6",
                "op": "contains",
                "value": "JavaScript"
              },
              "action": {
                "type": "append",
                "varName": "tech_stack",
                "value": "frontend"
              }
            },
            {
              "condition": {
                "path": "6",
                "op": "contains",
                "value": "Python"
              },
              "action": {
                "type": "append",
                "varName": "tech_stack",
                "value": "backend"
              }
            }
          ]
        },
        {
          "id": 7,
          "title": "Is Senior Developer",
          "type": "boolean",
          "value": false,
          "visibleWhen": {
            "path": "6",
            "op": "contains",
            "value": "JavaScript"
          }
        },
        {
          "id": 8,
          "title": "Years of Experience",
          "type": "numeric",
          "value": "",
          "placeholder": "Enter years of experience",
          "visibleWhen": {
            "path": "7",
            "op": "equals",
            "value": true
          },
          "onCondition": [
            {
              "condition": {
                "path": "8",
                "op": "greater_than",
                "value": "5"
              },
              "action": {
                "type": "set",
                "varName": "experience_level",
                "value": "senior"
              }
            },
            {
              "condition": {
                "path": "8",
                "op": "lessThanOrEqual",
                "value": "5"
              },
              "action": {
                "type": "set",
                "varName": "experience_level",
                "value": "mid"
              }
            },
            {
              "condition": {
                "allOf": [
                  {
                    "path": "8",
                    "op": "greater_than",
                    "value": "10"
                  },
                  {
                    "path": "7",
                    "op": "equals",
                    "value": true
                  }
                ]
              },
              "action": {
                "type": "append",
                "varName": "user_tags",
                "value": "expert"
              }
            }
          ]
        },
        {
          "id": "title",
          "title": "Additional Information",
          "type": "title",
          "value": "Please provide any additional information"
        },
        {
          "id": 9,
          "title": "Birth Date",
          "type": "date",
          "value": ""
        },
        {
          "id": 10,
          "title": "Welcome Message",
