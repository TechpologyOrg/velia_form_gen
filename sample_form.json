        },
        {
          "id": 2,
          "title": "Email Address",
          "type": "text",
          "value": "",
          "placeholder": "Enter your email",
          "onCondition": [
            {
              "condition": {
                "path": "2",
                "op": "isNotEmpty",
                "value": ""
